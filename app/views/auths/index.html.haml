%p#notice= notice
%h1 Listing Auths
%table
  %thead
    %tr
      %th User
      %th Key
      %th Secret
      %th{:colspan => "3"}
  %tbody
    - @auths.each do |auth|
      %tr
        %td= auth.user
        %td= auth.key
        %td= auth.secret
        %td= link_to 'Show', auth
        %td= link_to 'Edit', edit_auth_path(auth)
        %td= link_to 'Destroy', auth, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Auth', new_auth_path
